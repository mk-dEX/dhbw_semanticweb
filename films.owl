<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY project "https://github.com/seaneble/dhbw_semanticweb#">
]>

<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:movieontology="http://www.movieontology.org/2009/10/01/movieontology.owl#">
    
    <!-- Movies -->
    
	<movieontology:Movie rdf:about="&project;Titanic">
		<movieontology:title>Titanic</movieontology:title>
	</movieontology:Movie>
    
	<movieontology:Movie rdf:about="&project;The-Great-Gatsby">
		<movieontology:title>The Great Gatsby</movieontology:title>
	</movieontology:Movie>
	
	<!-- Actors -->
	
	<movieontology:Actor rdf:about="&project;Leonardo-DiCaprio">
    	<movieontology:name>Leonardo DiCaprio</movieontology:name>
    	<movieontology:isActorIn rdf:resource="&project;Titanic" />
    	<movieontology:isActorIn rdf:resource="&project;The-Great-Gatsby" />
	</movieontology:Actor>
	
	<movieontology:Actor rdf:about="&project;Kate-Winslet">
    	<movieontology:name>Kate Winslet</movieontology:name>
    	<movieontology:isActressIn rdf:resource="&project;Titanic" />
	</movieontology:Actor>

</rdf:RDF>
